window {
    size(168,20)

    bind('analog','analog')

    panel('','vertical') {
        bgthickness(2)
        togglebutton('analog','Analog mode') {
            channel('analog')
            desired(160,16)
            tooltips('Output the same power','level as the input, instead','of always 15 or 0')
            check(true)
        }
    }
}
